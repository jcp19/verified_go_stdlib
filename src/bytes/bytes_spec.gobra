package bytes

// +gobra

import (
	sl "verification/utils/slices"
)

ghost
requires acc(sl.Bytes(ub, 0, len(ub)), _)
ensures len(res) == len(ub)
ensures forall i int :: { res[i] } 0 <= i && i < len(ub) ==>
    res[i] == sl.GetByte(ub, 0, len(ub), i)
decreases _
pure func View(ub []byte) (res seq[byte])
