decreases len(s)
func lemmaSplitInnerAddNonSep(s, ac seq[byte], add byte, res1, res2 seq[seq[byte]]) {
	sep := seq[byte]{'/'}
	added := s ++ seq[byte]{add}
	switch {
	case s == sep:
		assert added == seq[byte]{'/', add}

		assert bytes.SplitInner(seq[byte]{}, sep, seq[byte]{}) == seq[seq[byte]]{seq[byte]{}}
		assert sep[1:] == seq[byte]{}
		assert res1 == seq[seq[byte]]{ ac, seq[byte]{} }

		assert bytes.SplitInner(added, sep, ac) == seq[seq[byte]]{ac} ++ bytes.SplitInner(added[1:], sep, seq[byte]{} )
		assert added[1:] == seq[byte]{add}
		assert bytes.SplitInner(added[1:], sep, seq[byte]{}) == bytes.SplitInner(seq[byte]{add}, sep, seq[byte]{})
		assert seq[byte]{add} != sep
		assert seq[byte]{add}[:1] != sep
		assert seq[byte]{add}[1:] == seq[byte]{}
		assert bytes.SplitInner(seq[byte]{add}, sep, seq[byte]{}) == bytes.SplitInner(seq[byte]{}, sep, seq[byte]{add})
		assert bytes.SplitInner(seq[byte]{}, sep, seq[byte]{add}) == seq[seq[byte]]{ seq[byte]{add} }

		assert res2 == seq[seq[byte]]{ac, seq[byte]{add}}

		assert res1[len(res1)-1] ++ seq[byte]{add} == res2[len(res2)-1]
		assert res1[:len(res1)-1] == res2[:len(res2)-1]
	case s[:len(sep)] == sep:


		assert res1 == seq[seq[byte]]{ac} ++ bytes.SplitInner(s[len(sep):], sep, seq[byte]{})
		assert res2 == seq[seq[byte]]{ac} ++ bytes.SplitInner(added[len(sep):], sep, seq[byte]{})
		assert s[len(sep):] ++ seq[byte]{add} == added[len(sep):]
		r1 := bytes.SplitInner(s[len(sep):], sep, seq[byte]{})
		r2 := bytes.SplitInner(added[len(sep):], sep, seq[byte]{})

		lemmaSplitInnerAddNonSep(s[len(sep):], seq[byte]{}, add, r1, r2)


		assert res1[len(res1)-1] ++ seq[byte]{add} == res2[len(res2)-1]
		assert res1[:len(res1)-1] == res2[:len(res2)-1]
	case len(s) == 1:
		assert res1 == bytes.SplitInner(s[1:], sep, ac ++ seq[byte]{s[0]})
		assert res2 == bytes.SplitInner(added[1:], sep, ac ++ seq[byte]{added[0]})

		assert s[1:] == seq[byte]{}
		assert bytes.SplitInner(seq[byte]{}, sep, ac ++ seq[byte]{s[0]}) == seq[seq[byte]]{ ac ++ seq[byte]{s[0]} }
		assert res1 == seq[seq[byte]]{ ac ++ seq[byte]{s[0]} }

		assert added[1:] == seq[byte]{add}
		assert bytes.SplitInner(seq[byte]{add}, sep, ac ++ seq[byte]{added[0]}) == bytes.SplitInner(seq[byte]{add}[1:], sep, ac ++ seq[byte]{added[0]} ++ seq[byte]{added[1]} )


		assert res1[len(res1)-1] ++ seq[byte]{add} == res2[len(res2)-1]
		assert res1[:len(res1)-1] == res2[:len(res2)-1]
	default:

		assert len(s) >= 2

		assert res1 == bytes.SplitInner(s[1:], sep, ac ++ seq[byte]{s[0]})
		assert res2 == bytes.SplitInner(added[1:], sep, ac ++ seq[byte]{added[0]})
		
		r1 := bytes.SplitInner(s[1:], sep, ac ++ seq[byte]{s[0]})
		r2 := bytes.SplitInner(added[1:], sep, ac ++ seq[byte]{added[0]}) 

		assert s[1:] ++ seq[byte]{add} == added[1:]

		lemmaSplitInnerAddNonSep(s[1:], ac ++ seq[byte]{added[0]}, add, res1, res2)
		assert res1[len(res1)-1] ++ seq[byte]{add} == res2[len(res2)-1]
		assert res1[:len(res1)-1] == res2[:len(res2)-1]
	}
}



