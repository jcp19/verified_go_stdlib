pred Lazybuf(b *lazybuf, p perm) {
	0 < p &&
	acc(b) &&
	0 < len(b.s) &&
	(b.buf != nil ==> InRangeInc(b.w, 0, len(b.buf))) &&
	InRangeInc(b.w, 0, len(b.s)) &&
	(b.buf == nil || len(b.buf) == len(b.s)) &&
	acc(sl.Bytes(b.s, 0, len(b.s)), p) &&
	sl.Bytes(b.buf, 0, len(b.buf))
}

