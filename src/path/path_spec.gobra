decreases
func lemmaToPathHasTail(p seq[byte]) {
	// lemmaToPathWithoutTrailingSlash(p)
	// lemmaSplitNoTrailingSlash(p)


	add := p[len(p)-1]
	s := p[:len(p)-1]
	sep := seq[byte]{'/'}

	if len(p) == 1 {
		assert pathContents(p) == p
		assert isRooted(p) == false
		assert bytes.Split(p, sep) == bytes.SplitInner(p, sep, seq[byte]{})
		assert p != sep
		assert p[:1] != sep
		assert bytes.SplitInner(p, sep, seq[byte]{}) == bytes.SplitInner(p[1:], sep, seq[byte]{ p[0] })
		assert p[1:] == seq[byte]{}
		assert bytes.SplitInner(seq[byte]{}, sep, seq[byte]{ p[0] }) == seq[seq[byte]]{ seq[byte]{ p[0] } }
		assert seq[byte]{ p[0] } == p
		assert bytes.Split(p, sep) == seq[seq[byte]]{ p }
		
	} else {
		res1 := bytes.Split(s, seq[byte]{'/'})
		assert p == s ++ seq[byte]{add}
		res2 := bytes.Split(p, seq[byte]{'/'})
		lemmaSplitAddNonSep(s, add, res1, res2)
	}
}



